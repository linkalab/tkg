<rdf:RDF xmlns="http://purl.org/acco/ns#"
     xml:base="http://purl.org/acco/ns"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:v1="http://purl.org/goodrelations/v1#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:acco="http://purl.org/acco/ns#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:dcterms="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="http://purl.org/acco/ns#">
        <owl:imports rdf:resource="http://purl.org/goodrelations/v1"/>
        <dcterms:contributor xml:lang="en">Michael Fried, Birgit Leiter, and Dieter Fensel. Some class and property definitions are inspired by or based on abstracts from Wikipedia, the free encyclopedia.</dcterms:contributor>
        <dcterms:creator xml:lang="en">Martin Hepp (Hepp Research GmbH)</dcterms:creator>
        <dcterms:license rdf:resource="http://creativecommons.org/licenses/by/3.0/"/>
