    <!-- http://purl.org/acco/ns#Accommodation -->

    <owl:Class rdf:about="http://purl.org/acco/ns#Accommodation">
        <rdfs:subClassOf rdf:resource="http://purl.org/goodrelations/v1#Location"/>
        <rdfs:subClassOf rdf:resource="http://purl.org/goodrelations/v1#ProductOrService"/>
        <rdfs:subClassOf rdf:resource="http://schema.org/Place"/>
        <rdfs:subClassOf rdf:resource="http://schema.org/Product"/>
        <rdfs:comment xml:lang="en">An accommodation is a place that can accommodate human beings, e.g. a hotel room, a camping pitch, or a meeting room. Many accommodations are for overnight stays, but this is not a mandatory requirement.

Note: For more specific types of accommodations not defined in this ontology, you can easily use any Wikipedia entry from 

    http://en.wikipedia.org/wiki/List_of_human_habitation_forms

and replace the base URI

    http://en.wikipedia.org/wiki/

by

    http://www.productontology.org/id/

For instance, if you want want to describe an igloo (http://en.wikipedia.org/wiki/Igloo) for rent, you can use the local part &apos;Igloo&apos; as a class identifier as follows:

    http://www.productontology.org/id/Igloo</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/acco/ns#"/>
        <rdfs:label xml:lang="en">Accommodation (gr:ProductOrService, gr:Location)</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/acco/ns#AccommodationFeature -->

    <owl:Class rdf:about="http://purl.org/acco/ns#AccommodationFeature">
        <rdfs:comment xml:lang="en">An accommodation feature is a structured value representing the feature of an accommodation as a property-value pair of varying degrees of formality.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/acco/ns#"/>
        <rdfs:label xml:lang="en">Accommodation Feature</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/acco/ns#Allinclusive -->

    <owl:Class rdf:about="http://purl.org/acco/ns#Allinclusive">
        <rdfs:subClassOf rdf:resource="http://purl.org/goodrelations/v1#ProductOrService"/>
        <rdfs:subClassOf rdf:resource="http://schema.org/Product"/>
        <rdfs:comment xml:lang="en">All-inclusive in the sense of this vocabulary is a comprehesive set of foods and beverages, and sometimes complementing services, provided throughout the entire day.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/acco/ns#"/>
        <rdfs:label xml:lang="en">All-inclusive (gr:ProductOrService)</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/acco/ns#Apartment -->

    <owl:Class rdf:about="http://purl.org/acco/ns#Apartment">
        <rdfs:subClassOf rdf:resource="http://purl.org/acco/ns#Accommodation"/>
        <rdfs:comment xml:lang="en">An apartment (in American English) or flat (in British English) is a self-contained housing unit (a type of residential real estate) that occupies only part of a building. (Source: Wikipedia, the free encyclopedia, see http://en.wikipedia.org/wiki/Apartment)</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/acco/ns#"/>
        <rdfs:label xml:lang="en">Apartment (gr:ProductOrService, gr:Location)</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/acco/ns#BedDetails -->

    <owl:Class rdf:about="http://purl.org/acco/ns#BedDetails">
        <rdfs:comment xml:lang="en">A node holding detailed information about the available bed types, e.g. the quantity of twin beds for a hotel room. For the single case of just one bed of a certain type, you can use acco:bed directly with acco:BedType instances.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/acco/ns#"/>
        <rdfs:label xml:lang="en">Bed Details</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/acco/ns#BedType -->

    <owl:Class rdf:about="http://purl.org/acco/ns#BedType">
        <rdfs:subClassOf rdf:resource="http://purl.org/goodrelations/v1#QualitativeValue"/>
        <rdfs:comment xml:lang="en">A type of bed. This is used for indicating the bed or beds available in an accommodation. Use DBpedia URIs whenever possible.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/acco/ns#"/>
        <rdfs:label xml:lang="en">Bed Type</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/acco/ns#Breakfast -->

    <owl:Class rdf:about="http://purl.org/acco/ns#Breakfast">
        <rdfs:subClassOf rdf:resource="http://purl.org/acco/ns#Meal"/>
        <rdfs:comment xml:lang="en">A breakfast is the first meal taken after rising from a night&apos;s sleep, most often eaten in the early morning before undertaking the day&apos;s work. (Source: Wikipedia, the free encyclopedia, see http://en.wikipedia.org/wiki/Breakfast)</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/acco/ns#"/>
        <rdfs:label xml:lang="en">Breakfast (gr:ProductOrService)</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/acco/ns#CampingPitch -->

    <owl:Class rdf:about="http://purl.org/acco/ns#CampingPitch">
        <rdfs:subClassOf rdf:resource="http://purl.org/acco/ns#Accommodation"/>
        <rdfs:comment xml:lang="en">A camping pitch is an individual place for overnight stay in the outdoors, typically being part of a larger acco:CampSite.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/acco/ns#"/>
        <rdfs:label xml:lang="en">Camping Pitch (gr:ProductOrService, gr:Location)</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/acco/ns#CampingSite -->

    <owl:Class rdf:about="http://purl.org/acco/ns#CampingSite">
        <rdfs:subClassOf rdf:resource="http://purl.org/acco/ns#Accommodation"/>
        <rdfs:comment xml:lang="en">A camping site, campsite, or campground is a place used for overnight stay in the outdoors. In British English a campsite is an area, usually divided into a number of pitches, where people can camp overnight using tents or camper vans or caravans; this British English use of the word is synonymous with the American English expression campground. In American English the term campsite generally means an area where an individual, family, group, or military unit can pitch a tent or parks a camper; a campground may contain many campsites. (Source: Wikipedia, the free encyclopedia, see http://en.wikipedia.org/wiki/Campsite)</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/acco/ns#"/>
        <rdfs:label xml:lang="en">Camping Site (gr:ProductOrService, gr:Location)</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/acco/ns#CompoundPrice -->

    <owl:Class rdf:about="http://purl.org/acco/ns#CompoundPrice">
        <rdfs:subClassOf rdf:resource="http://purl.org/goodrelations/v1#PriceSpecification"/>
        <rdfs:comment xml:lang="en">A compound price specification is one that bundles multiple prices that all apply in combination for different dimensions of consumption. Use gr:name for indicating the dimension (e.g. &quot;electricity&quot; or &quot;final cleaning&quot;).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/acco/ns#"/>
        <rdfs:label xml:lang="en">Compound Price</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/acco/ns#Dinner -->

    <owl:Class rdf:about="http://purl.org/acco/ns#Dinner">
        <rdfs:subClassOf rdf:resource="http://purl.org/acco/ns#Meal"/>
        <rdfs:comment xml:lang="en">A dinner is the main meal of the day, often consumed in the evening of the day.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/acco/ns#"/>
        <rdfs:label xml:lang="en">Dinner (gr:ProductOrService)</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/acco/ns#DwellingType -->

    <owl:Class rdf:about="http://purl.org/acco/ns#DwellingType">
        <rdfs:subClassOf rdf:resource="http://purl.org/goodrelations/v1#QualitativeValue"/>
        <rdfs:comment xml:lang="en">A type of dwelling, e.g. tent, mobile home, etc. for indicating the permitted usage on a campsite. Use DBpedia URIs whenever possible.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/acco/ns#"/>
        <rdfs:label xml:lang="en">Dwelling Type</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/acco/ns#Hotel -->

    <owl:Class rdf:about="http://purl.org/acco/ns#Hotel">
        <rdfs:subClassOf rdf:resource="http://purl.org/acco/ns#Accommodation"/>
        <rdfs:comment xml:lang="en">A hotel is an establishment that provides lodging paid on a short-term basis. (Source: Wikipedia, the free encyclopedia, see http://en.wikipedia.org/wiki/Hotel)</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/acco/ns#"/>
        <rdfs:label xml:lang="en">Hotel (gr:ProductOrService, gr:Location)</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/acco/ns#HotelRoom -->

    <owl:Class rdf:about="http://purl.org/acco/ns#HotelRoom">
        <rdfs:subClassOf rdf:resource="http://purl.org/acco/ns#Accommodation"/>
        <rdfs:comment xml:lang="en">A hotel room is a single room in a hotel.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/acco/ns#"/>
        <rdfs:label xml:lang="en">Hotel Room (gr:ProductOrService, gr:Location)</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/acco/ns#House -->

    <owl:Class rdf:about="http://purl.org/acco/ns#House">
        <rdfs:subClassOf rdf:resource="http://purl.org/acco/ns#Accommodation"/>
        <rdfs:comment xml:lang="en">A house is a building or structure that has the ability to be occupied for habitation by humans or other creatures. (Source: Wikipedia, the free encyclopedia, see http://en.wikipedia.org/wiki/House)</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/acco/ns#"/>
        <rdfs:label xml:lang="en">House (gr:ProductOrService, gr:Location)</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/acco/ns#Lunch -->

    <owl:Class rdf:about="http://purl.org/acco/ns#Lunch">
        <rdfs:subClassOf rdf:resource="http://purl.org/acco/ns#Meal"/>
        <rdfs:comment xml:lang="en">A luncheon, commonly abbreviated to lunch, is a middle of the day meal. (Source: Wikipedia, the free encyclopedia, see http://en.wikipedia.org/wiki/Lunch)</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/acco/ns#"/>
        <rdfs:label xml:lang="en">Lunch (gr:ProductOrService)</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/acco/ns#Meal -->

    <owl:Class rdf:about="http://purl.org/acco/ns#Meal">
        <rdfs:subClassOf rdf:resource="http://purl.org/goodrelations/v1#ProductOrService"/>
        <rdfs:subClassOf rdf:resource="http://schema.org/Product"/>
        <rdfs:comment xml:lang="en">A meal in the sense of this vocabulary is some combination of food or beverages.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/acco/ns#"/>
        <rdfs:label xml:lang="en">Meal (gr:ProductOrService)</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/acco/ns#MeetingRoom -->

    <owl:Class rdf:about="http://purl.org/acco/ns#MeetingRoom">
        <rdfs:subClassOf rdf:resource="http://purl.org/acco/ns#Accommodation"/>
        <rdfs:comment xml:lang="en">A meeting room, conference room, or conference hall is a room provided for singular events such as business conferences and meetings. (Source: Wikipedia, the free encyclopedia, see http://en.wikipedia.org/wiki/Conference_hall)  </rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/acco/ns#"/>
        <rdfs:label xml:lang="en">Meeting Room (gr:ProductOrService, gr:Location)</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/acco/ns#Resort -->

    <owl:Class rdf:about="http://purl.org/acco/ns#Resort">
        <rdfs:subClassOf rdf:resource="http://purl.org/acco/ns#Accommodation"/>
        <rdfs:comment xml:lang="en">A resort is a place used for relaxation or recreation, attracting visitors for holidays or vacations. Resorts are places, towns or sometimes commercial establishment operated by a single company. (Source: Wikipedia, the free encyclopedia, see http://en.wikipedia.org/wiki/Resort)</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/acco/ns#"/>
        <rdfs:label xml:lang="en">Resort (gr:ProductOrService, gr:Location)</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/acco/ns#Suite -->

    <owl:Class rdf:about="http://purl.org/acco/ns#Suite">
        <rdfs:subClassOf rdf:resource="http://purl.org/acco/ns#Accommodation"/>
        <rdfs:comment xml:lang="en">A suite in a hotel or other public accommodation, denotes a class of luxury accommodations, the key feature of which is multiple rooms. (Source: Wikipedia, the free encyclopedia, see http://en.wikipedia.org/wiki/Suite_(hotel)) </rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/acco/ns#"/>
        <rdfs:label xml:lang="en">Suite (gr:ProductOrService, gr:Location)</rdfs:label>
    </owl:Class>
    


