        <testannotationschema:hasSPARQLQueryUnitTest># CQ10: In which months do we have the highest number of user reviews for Hotels?

PREFIX kgt: &lt;http://tourism.kg.linkalab.it/&gt;
PREFIX tao: &lt;http://purl.org/tao/ns#&gt;
PREFIX acco: &lt;http://purl.org/acco/ns#&gt;
PREFIX rdf: &lt;http://www.w3.org/1999/02/22-rdf-syntax-ns#&gt;
PREFIX schema: &lt;http://schema.org/&gt;

select ?month (COUNT(DISTINCT ?r) AS ?tot_reviews) 
where { 
        ?lodge a tao:LodgingFacility ;
        a tao:Hotel ;
        schema:review ?r .
		?r schema:dateCreated ?date .
    BIND (month(?date) as ?month)
        
}
GROUP BY ?month
ORDER BY DESC(?tot_reviews)
limit 1</testannotationschema:hasSPARQLQueryUnitTest>
