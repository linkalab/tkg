        <testannotationschema:hasSPARQLQueryUnitTest># CQ12: How many beds are offered on lease in a certain Tourist Destination?

PREFIX tao: &lt;http://purl.org/tao/ns#&gt;
PREFIX acco: &lt;http://purl.org/acco/ns#&gt;
PREFIX xsd: &lt;http://www.w3.org/2001/XMLSchema#&gt;
PREFIX kgt_td: &lt;http://tourism.kg.linkalab-cloud.com/td/&gt;

select ?dst (xsd:integer(SUM(xsd:float(?num))) as ?total_beds_on_lease)
where { 
        ?ac a 	tao:Accommodation ;
            	tao:partOf ?lodge;
        		acco:bed ?bed .
        ?lodge	tao:isContainedInGeo ?dst .
    	?dst a tao:TouristDestination .
    	?bed acco:quantity ?num . 
    FILTER(datatype(?num) = xsd:integer)
    FILTER(?dst = kgt_td:sardinia)
} group by ?dst</testannotationschema:hasSPARQLQueryUnitTest>
